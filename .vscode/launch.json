  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Server",
      "request": "launch",
      "runtimeArgs": ["run-script", "debug-development-server"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },

    {
      "name": "Debug UI",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/ui/src"
    }
    // {
    //   "name": "Debug UI",
    //   "request": "launch",
    //   "runtimeArgs": ["run-script", "debug-ui"],
    //   "runtimeExecutable": "npm",
    //   "skipFiles": ["<node_internals>/**"],
    //   "type": "node"
    // }
  ]
}
